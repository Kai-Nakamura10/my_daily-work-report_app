<%= form_with url: login_path, scope: :session, local: true do |f| %>
  <div>
    <%= f.label :email, "メールアドレス" %><br>
    <%= f.email_field :email %>
  </div>
  <div>
    <%= f.label :password, "パスワード" %><br>
    <%= f.password_field :password %>
  </div>
  <div>
    <%= f.submit "ログイン" %>
  </div>
<% end %>
